    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-lodash-template": "^0.21.0",
    "graphql": "^16.8.1",
    "graphql-sse": "^2.3.0",
    "graphql-ws": "^5.14.0",
