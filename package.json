  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quasarframework/app-extension-apollo"
  },
  "bugs": "https://github.com/quasarframework/app-extension-apollo/issues",
  "homepage": "https://quasar.dev",
  "scripts": {
